generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Entities {
  id Int @id @default(autoincrement())
  equipment String
  type_of_fault String
  description_problem String
  client String
  status String
  status_update Boolean @default(false)
  master String
  date DateTime
}